// Enter your template code here.
const log = require('logToConsole');
const setInWindow = require('setInWindow');
log('data =', data);

const configObject = {};

 /* General */
if (data.customUtiqHost) {
  configObject.customUtiqHost = data.customUtiqHost;
}
if (data.cmpName) {
  configObject.CMP = data.cmpName;
}
if (data.variation) {
  configObject.variation = data.variation;
}
if (data.language) {
  configObject.customizationOptions = {}; 
  configObject.customizationOptions.defaultLanguage = data.language;
}
if (data.getNewConsentBeforeExpiry === true) {
  configObject.getNewConsentBeforeExpiry = false;
}
if (data.connectionEligibilityRetryInterval) {
  configObject.connectionEligibilityRetryInterval = data.connectionEligibilityRetryInterval;
}

if (data.disableGoogleSecureSignals) {
  configObject.integrations = {}; 
  configObject.integrations.disableGoogleSecureSignals = data.disableGoogleSecureSignals;
}
/* Consent manager */
if (data.consentManagerOrigin) {
  configObject.consentManagerOrigin = data.consentManagerOrigin;
}
if (data.consentManagerDataLayer) {
  configObject.consentManagerDataLayer = data.consentManagerDataLayer;
}
/* CMP */
if (data.UtiqVendorId && data.UtiqVendorId) {
  if (data.cmpName === 'Optanon') {
    /* Optanon/OneTrust */
    configObject.cmpUtiqPurposeId = data.UtiqPurposeId;
    configObject.cmpUtiqVendorId = data.UtiqVendorId;
  } else if (data.cmpName === 'Didomi') {
    /* Didomi */
    configObject.didomiUtiqPurposeId = data.UtiqPurposeId;
    configObject.didomiUtiqVendorId = data.UtiqVendorId;  
  }
}
log('configObject: ', configObject);
setInWindow('Utiq', { 
  config: configObject  
});

// Call data.gtmOnSuccess when the tag is finished.
data.gtmOnSuccess();


